    "def get_ordered_colors(colormap, n):\n",
    "    colors = []\n",
    "    cmap = plt.cm.get_cmap(colormap)\n",
    "    for colorVal in np.linspace(0, 1, n):\n",
    "        colors.append(cmap(colorVal))\n",
    "    return colors"
   ]
